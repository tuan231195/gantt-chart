  "main": "index.js",
  "scripts": {
    "start": "budo index.js:bundle.js --live --open -- -t [ babelify --presets [ env ] --sourceType module ]",
    "bundle": "mkdir -p dist && browserify index.js -t [ babelify --presets [ env ] --sourceType module ] | uglifyjs > dist/bundle.js && cp index.html gantt.css dist/",
    "upload": "surge dist"
  },
