  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babelify": "8.0.0",
    "budo": "11.2.0",
    "surge": "0.20.1",
    "uglify-js": "3.3.21"
  },
  "dependencies": {
    "d3": "4.13.0"
  }
}
